@layer theme {

  :root,
  :host {
    /* Typography - Original System Stack */
    --font-sans: ui-sans-serif, system-ui, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "Noto Color Emoji";
    --font-mono: ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, "Liberation Mono", "Courier New", monospace;

    /* Colors - Restoring Base */
    --color-background: #FFFFFF;
    --color-foreground: #000000;

    --color-primary: #000000;
    --color-primary-hover: #333333;

    --color-accent-gold: #B68D40;
    --color-accent-gold-light: #D4B886;

    --color-gray-50: #FBFBFD;
    --color-gray-100: #F5F5F7;
    --color-gray-200: #E5E5EA;
    --color-gray-300: #D1D1D6;
    --color-gray-400: #C7C7CC;
    --color-gray-500: #AEAEB2;
    --color-gray-600: #8E8E93;
    --color-gray-700: #636366;
    --color-gray-800: #48484A;
    --color-gray-900: #1C1C1E;

    /* Spacing */
    --spacing: 0.25rem;

    /* Shadows - Layered & Smooth */
    --shadow-sm: 0 1px 2px rgba(0, 0, 0, 0.04);
    --shadow-md: 0 4px 12px rgba(0, 0, 0, 0.06), 0 1px 2px rgba(0, 0, 0, 0.04);
    --shadow-lg: 0 12px 24px rgba(0, 0, 0, 0.08), 0 4px 8px rgba(0, 0, 0, 0.04);
    --shadow-xl: 0 24px 48px rgba(0, 0, 0, 0.12), 0 8px 16px rgba(0, 0, 0, 0.04);
    --shadow-gold: 0 8px 24px rgba(182, 141, 64, 0.25), 0 4px 8px rgba(182, 141, 64, 0.15);

    /* Radius */
    --radius-sm: 0.5rem;
    --radius-md: 0.75rem;
    --radius-lg: 1rem;
    --radius-xl: 1.5rem;
    --radius-2xl: 2rem;
    --radius-full: 9999px;

    /* Animations */
    --ease-out: cubic-bezier(0.25, 0.1, 0.25, 1);
    --ease-in-out: cubic-bezier(0.42, 0, 0.58, 1);
    --spring: cubic-bezier(0.175, 0.885, 0.32, 1.275);
  }
}

@layer utilities {
  .glass {
    background: rgba(255, 255, 255, 0.7);
    backdrop-filter: blur(20px) saturate(180%);
    -webkit-backdrop-filter: blur(20px) saturate(180%);
    border: 1px solid rgba(255, 255, 255, 0.3);
  }

  .glass-dark {
    background: rgba(28, 28, 30, 0.7);
    backdrop-filter: blur(20px) saturate(180%);
    -webkit-backdrop-filter: blur(20px) saturate(180%);
    border: 1px solid rgba(255, 255, 255, 0.1);
  }

  .text-balance {
    text-wrap: balance;
  }

  .text-gradient-gold {
    background: linear-gradient(135deg, #B68D40 0%, #D4B886 50%, #B68D40 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    background-size: 200% auto;
    animation: shine 4s linear infinite;
  }

  .text-gradient-premium {
    background: linear-gradient(135deg, #1C1C1E 0%, #3A3A3C 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
  }

  .bg-gradient-subtle {
    background: radial-gradient(circle at 50% 0%, rgba(0, 0, 0, 0.02) 0%, rgba(255, 255, 255, 0) 70%);
  }

  .glass-premium {
    background: rgba(255, 255, 255, 0.65);
    backdrop-filter: blur(24px) saturate(180%);
    -webkit-backdrop-filter: blur(24px) saturate(180%);
    border: 1px solid rgba(0, 0, 0, 0.04);
    box-shadow: 0 4px 24px rgba(0, 0, 0, 0.02);
  }

  @keyframes shine {
    to {
      background-position: 200% center;
    }
  }

  @keyframes float {

    0%,
    100% {
      transform: translateY(0);
    }

    50% {
      transform: translateY(-10px);
    }
  }

  .animate-float {
    animation: float 6s ease-in-out infinite;
  }
}

